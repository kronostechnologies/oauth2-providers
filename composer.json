{
  "name": "kronos/oauth2-providers",
  "type": "library",
  "description": "Kronos OAuth2 providers for https://github.com/thephpleague/oauth2-client",
  "homepage": "https://github.com/kronostechnologies/oauth2-providers",
  "license": "MIT",
  "keywords": [
    "kronos",
    "oauth2",
    "oauth2-client",
    "provider"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\OAuth2Providers\\": "Kronos/OAuth2Providers"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kronostechnologies/oauth2-microsoft-graph.git"
    }
  ],
  "require": {
    "php": ">=7.2",
    "league/oauth2-client": "^2.4.1",
    "league/oauth2-google":"^3.0.2",
    "kronos/oauth2-microsoft-graph": "^0.2.0",
    "phpseclib/phpseclib": "^3.0",
    "firebase/php-jwt": "^5.0",
    "stevenmaguire/oauth2-microsoft": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.20"
  }
}
