{
  "name": "kronos/oauth2-providers",
  "type": "library",
  "description": "Kronos OAuth2 providers for https://github.com/thephpleague/oauth2-client",
  "homepage": "https://github.com/kronostechnologies/oauth2-providers",
  "license": "MIT",
  "keywords": [
    "kronos",
    "oauth2",
    "oauth2-client",
    "provider"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\OAuth2Providers\\": "Kronos/OAuth2Providers",
      "Kronos\\Tests\\OAuth2Providers\\": "tests/Kronos/Tests/OAuth2Providers"
    }
  },
  "require": {
    "php": "^8.1",
    "league/oauth2-client": "^2.7.0",
    "league/oauth2-google": "^4.0.1",
    "firebase/php-jwt": "^6.5.0",
    "guzzlehttp/psr7": "^2.5.0",
    "thenetworg/oauth2-azure": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "10.5.9",
    "vimeo/psalm": "5.20.0",
    "cyclonedx/cyclonedx-php-composer": "5.0.0",
    "kronostechnologies/php-coding-standard": "v2.0.0",
    "squizlabs/php_codesniffer": "3.8.1"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "cyclonedx/cyclonedx-php-composer": true
    }
  }
}
