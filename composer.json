{
  "name": "kronos/oauth2-providers",
  "type": "library",
  "description": "Kronos OAuth2 providers for https://github.com/thephpleague/oauth2-client",
  "homepage": "https://github.com/kronostechnologies/oauth2-providers",
  "license": "MIT",
  "keywords": [
    "kronos",
    "oauth2",
    "oauth2-client",
    "provider"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\OAuth2Providers\\": "Kronos/OAuth2Providers"
    }
  },
  "require": {
    "php": "^5.6",
    "league/oauth2-client": "^2.2",
    "league/oauth2-google":"^2.0",
    "stevenmaguire/oauth2-microsoft" : "^2.0",
    "phpseclib/phpseclib": "^2.0",
    "firebase/php-jwt": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.3.0"
  }
}
