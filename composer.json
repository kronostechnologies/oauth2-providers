{
  "name": "kronos/oauth2-providers",
  "type": "library",
  "description": "Kronos OAuth2 providers for https://github.com/thephpleague/oauth2-client",
  "homepage": "https://github.com/kronostechnologies/oauth2-providers",
  "license": "MIT",
  "keywords": [
    "kronos",
    "oauth2",
    "oauth2-client",
    "provider"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\OAuth2Providers\\": "Kronos/OAuth2Providers",
      "Kronos\\Tests\\OAuth2Providers\\": "tests/Kronos/Tests/OAuth2Providers"
    }
  },
  "require": {
    "php": "^7.4 || ^8.1",
    "league/oauth2-client": "^2.6.1",
    "league/oauth2-google":"^4.0.0",
    "firebase/php-jwt": "^6.2",
    "guzzlehttp/psr7": "^1.8.5 || ^2.2.1",
    "thenetworg/oauth2-azure": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.21",
    "vimeo/psalm": "4.26.0",
    "cyclonedx/cyclonedx-php-composer": "3.10.0",
    "kronostechnologies/php-coding-standard": "2.0.0",
    "squizlabs/php_codesniffer": "3.7.1"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "cyclonedx/cyclonedx-php-composer": true
    }
  }
}
