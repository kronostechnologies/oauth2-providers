{
  "name": "kronos/oauth2-providers",
  "type": "library",
  "description": "Kronos OAuth2 providers for https://github.com/thephpleague/oauth2-client",
  "homepage": "https://github.com/kronostechnologies/oauth2-providers",
  "license": "MIT",
  "keywords": [
    "kronos",
    "oauth2",
    "oauth2-client",
    "provider"
  ],
  "autoload": {
    "psr-4": {
      "Kronos\\Oauth2Providers\\": "Kronos/Oauth2Providers"
    }
  },
  "require": {
    "php": "^5.6",
    "league/oauth2-client": "*",
    "league/oauth2-google":"*",
    "thenetworg/oauth2-azure":"dev-master",
    "stevenmaguire/oauth2-microsoft":"*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*"
  }
}